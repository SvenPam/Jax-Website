{
  "name": "jaxwebsite",
  "version": "0.0.1",
  "description": "Jax Website",
  "main": "null",
  "scripts": {
    "build": "gulp",
    "watch": "gulp watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Schteele/Jax-Website.git"
  },
  "keywords": [
    "umbraco",
    "personal"
  ],
  "author": "Jacquie Steele",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.5.4",
    "babel-preset-es2015": "^6.22.0",
    "bootstrap": "^4.1.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^6.1.2",
    "gulp-changed": "^1.3.2",
    "gulp-clean-css": "^2.3.2",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "^3.1.1",
    "gulp-jshint": "^2.0.4",
    "gulp-json-editor": "^2.4.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-postcss": "^6.2.0",
    "gulp-rename": "^1.3.0",
    "gulp-sass": "^3.2.1",
    "gulp-sass-lint": "^1.4.0",
    "gulp-sourcemaps": "^1.9.1",
    "jquery": "^3.1.1",
    "jshint": "^2.9.4",
    "sass-lint": "^1.10.2"
  },
  "paths": {
    "src": {
      "styles": "./a/src/styles",
      "scripts": "./a/src/scripts",
      "images": "./a/src/images",
      "externalJs": "./a/src/scripts/external-references.json",
      "sprites": "./a/src/sprites"
    },
    "dist": {
      "styles": "./a/dist/css",
      "scripts": "./a/dist/js",
      "images": "./a/dist/img",
      "sprites": "./a/dist/sprites"
    }
  },
  "jshintConfig": {},
  "sassLintConfig": {
    "rules": {
      "no-ids": 2,
      "no-duplicate-properties": 2,
      "no-vendor-prefixes": 2,
      "no-color-literals": 2,
      "no-empty-rulesets": 2,
      "indentation": 0,
      "final-newline": 0,
      "force-pseudo-nesting": 0,
      "property-sort-order": 0,
      "class-name-format": 0,
      "placeholder-in-extend": 0,
      "no-css-comments": 0,
      "hex-notation": {
        "style": "uppercase"
      },
      "brace-style": {
        "style": "stroustrup"
      },
      "nesting-depth": {
        "max-depth": "3"
      }
    }
  }
}
